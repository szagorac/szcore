<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<BorderPane prefHeight="729.0" prefWidth="870.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.xenaksys.szcore.gui.view.DialogsScoreController">
    <center>
        <AnchorPane prefHeight="455.0" prefWidth="568.0" BorderPane.alignment="CENTER">
            <children>
                <VBox spacing="10.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
                    <children>
                        <HBox alignment="CENTER_LEFT" spacing="10.0">
                            <children>
                                <Label text="Sections:" />
                        <Label fx:id="sectionsStatusLbl" styleClass="label-red" stylesheets="@css/szcoreStyle.css" text="WAITING" />
                        <Button fx:id="assignOwnersRndBtn" mnemonicParsing="false" onAction="#assignSectionOwnersRnd" text="Assign Owners Rnd" />
                        <Button fx:id="resetOwnersRndBtn" layoutX="147.0" layoutY="11.0" mnemonicParsing="false" onAction="#resetSectionOwners" text="Reset Owners" />
                            </children>
                        </HBox>
                        <HBox spacing="10.0">
                            <children>
                                <TableView fx:id="sectionsTableView" prefHeight="200.0" prefWidth="443.0">
                                    <columns>
                                        <TableColumn fx:id="sectionColumn" maxWidth="200.0" minWidth="100.0" prefWidth="120.0" text="Section" />
                                        <TableColumn fx:id="ownerColumn" maxWidth="150.0" minWidth="80.0" prefWidth="100.0" text="Owner" />
                                        <TableColumn fx:id="startPageColumn" maxWidth="120.0" minWidth="30.0" prefWidth="70.0" text="Start Page" />
                                        <TableColumn fx:id="endPageColumn" maxWidth="120.0" minWidth="30.0" prefWidth="70.0" text="End Page" />
                                    </columns>
                                </TableView>
                        <ListView fx:id="sectionOrderLvw" orientation="HORIZONTAL" prefHeight="100.0" prefWidth="474.0" />
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </children>
        </AnchorPane>
    </center>
</BorderPane>
